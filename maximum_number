A=int(input("Enter the number A : "))
B=int(input("Enter the number B : "))
C=int(input("Enter the number C: "))
if A>B:
    if A>C:
        print('A is greater number among three')
if B>A:
    if B>C:
        print('B is greater number among three')
    else:
        print('C is greater number among three') 

